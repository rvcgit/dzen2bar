#!/usr/bin/env sh
# BOTTOMRIGHT ALIGN [undocked; to run with dzen2panel which is docked]
sleep 5 &
trayer --edge bottom --align right --transparent false \
--alpha 0 --widthtype request  --heighttype request --height 18 \
--iconspacing 4 --SetDockType false --SetPartialStrut false \
--tint 0x111827 --padding 1 --margin 2 --expand true
